<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <report
        id="report_car_workshop_products"
        string="Product Display Names (PDF)"
        model="product.template"
        report_type="qweb-pdf"
        name="car_workshop.report_most_demanded_service"
        file="car_workshop.report_most_demanded_service"
        print_report_name="'Products - %s' % (object.name)"
        menu="False"
    />

    <template id="report_most_demanded_service">
        <t t-call="web.basic_layout">
            <div class="page">
                <strong>
                <t t-esc="name" /> 
                <t t-esc="extras.get('age')" /> 
                </strong>
            </div>
        </t>
    </template>
</odoo>